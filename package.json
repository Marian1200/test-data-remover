  "icon": "TestExtensionIcon.png",
  "version": "0.1.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/Marian1200/test-data-remover.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
